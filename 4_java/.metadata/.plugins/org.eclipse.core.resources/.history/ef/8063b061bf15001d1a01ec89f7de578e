
public class TestaCaracteres {
	public static void main(String[] args) {
		
		char letra = 'a';
		System.out.println(letra); // a
		
		char valor = 66;
		System.out.println(valor); // B
		
		valor = (char)(valor + 1);
		System.out.println(valor); // C
		
		String palavra = "curso de Java";
		
		System.out.println(palavra);
				
		
	}
}
